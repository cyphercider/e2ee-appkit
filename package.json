{
  "name": "@cyphercider/e2ee-appkit",
  "repository": "https://github.com/cyphercider/e2ee-appkit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run prebuild && pnpm nx run-many --all --target=build",
    "test": "pnpm nx run-many --all --target=test --coverage",
    "lint": "pnpm nx run-many --all --target=lint --fix",
    "build-test-lint": "pnpm run build && pnpm run test && pnpm run lint",
    "build:e2ee-browser": "pnpm run prebuild && nx build e2ee-appkit-browser",
    "build:e2ee-shared-modules": "nx build e2ee-appkit-shared-models",
    "test:e2ee-browser": "nx test e2ee-appkit-browser --coverage",
    "lint:e2ee-browser": "nx lint e2ee-appkit-browser --fix",
    "test:e2ee-browser:report": "sensible-browser ./coverage/packages/e2ee-appkit-browser/index.html",
    "test:e2ee-node": "nx test e2ee-appkit-node --coverage",
    "lint:e2ee-node": "nx lint e2ee-appkit-node --fix",
    "test:e2ee-node:report": "sensible-browser ./coverage/packages/e2ee-appkit-node/index.html",
    "clean": "pnpm nx reset && rm -rf dist",
    "prebuild": "rm -rf dist",
    "login": "pnpm login",
    "build-and-publish-all": "pnpm run build && pnpm run publish-all",
    "publish:e2ee-browser": "pnpm publish ./dist/e2ee-appkit-browser",
    "publish:e2ee-node": "pnpm publish ./dist/e2ee-appkit-node",
    "publish:e2ee-shared-modules": "pnpm publish ./dist/e2ee-appkit-shared-models",
    "publish-all": "pnpm run publish:e2ee-browser && pnpm run publish:e2ee-node && pnpm run publish:e2ee-shared-modules",
    "bump-all-versions:patch": "./bump-version.sh patch",
    "bump-all-versions:minor": "./bump-version.sh minor",
    "bump-all-versions:major": "./bump-version.sh major"
  },
  "private": true,
  "devDependencies": {
    "@nx/eslint-plugin-nx": "16.0.0-beta.1",
    "@nx/jest": "17.2.0",
    "@nx/js": "17.2.0",
    "@nx/linter": "17.2.0",
    "@nx/node": "17.2.0",
    "@nx/web": "17.2.0",
    "@nx/workspace": "17.2.0",
    "@trellisorg/nxls": "0.0.19",
    "@types/jest": "^29",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "jest-environment-node": "^29",
    "nx": "17.2.0",
    "prettier": "^3.0.1",
    "ts-jest": "^29",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "axios": "1.4.0",
    "buffer": "6.0.3",
    "crypto-browserify": "3.12.0",
    "date-fns": "2.30.0",
    "jose": "4.14.4",
    "session-keystore": "1.0.3",
    "tslib": "^2.6.1",
    "ulid": "2.3.0"
  }
}

