{
  "name": "@cyphercider/e2ee-appkit",
  "repository": "https://github.com/cyphercider/e2ee-appkit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run prebuild && pnpm nx run-many --all --target=build",
    "test": "pnpm nx run-many --all --target=test --coverage",
    "lint": "pnpm nx run-many --all --target=lint --fix",
    "build:e2ee-browser": "pnpm run prebuild && nx build e2ee-appkit-browser",
    "build:e2ee-shared-modules": "nx build e2ee-appkit-shared-models",
    "test:e2ee-browser": "nx test e2ee-appkit-browser --coverage",
    "lint:e2ee-browser": "nx lint e2ee-appkit-browser --fix",
    "test:e2ee-browser:report": "sensible-browser ./coverage/packages/e2ee-appkit-browser/index.html",
    "test:e2ee-node": "nx test e2ee-appkit-node --coverage",
    "lint:e2ee-node": "nx lint e2ee-appkit-node --fix",
    "test:e2ee-node:report": "sensible-browser ./coverage/packages/e2ee-appkit-node/index.html",
    "clean": "pnpm nx reset && rm -rf dist",
    "prebuild": "rm -rf dist"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/eslint-plugin-nx": "15.8.5",
    "@nrwl/jest": "15.8.5",
    "@nrwl/js": "15.8.5",
    "@nrwl/linter": "15.8.5",
    "@nrwl/node": "15.8.5",
    "@nrwl/web": "15.8.5",
    "@nrwl/workspace": "15.8.5",
    "@trellisorg/nxls": "0.0.19",
    "@types/jest": "^29",
    "@types/node": "~18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "~8.35.0",
    "eslint-config-prettier": "8.7.0",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "jest-environment-node": "^29",
    "nx": "15.8.5",
    "prettier": "^2.8.4",
    "ts-jest": "^29",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "axios": "1.3.4",
    "buffer": "6.0.3",
    "crypto-browserify": "3.12.0",
    "date-fns": "2.29.3",
    "jose": "4.13.1",
    "session-keystore": "1.0.3",
    "tslib": "^2.5.0",
    "ulid": "2.3.0"
  }
}
